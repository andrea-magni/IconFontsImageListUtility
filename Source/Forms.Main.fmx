        '// https://github.com/andrea-magni/IconFontsImageListUtility'
        '// %TimeStamp%'
        ''
        '{$if compilerversion >= 21}'
        '  {$define Delphi2010_UP}'
        '{$ifend}'
        ''
        'type'
        '  %TypeName% = record // %Count% entries'
        '{$IFDEF Delphi2010_UP}'
        '  private'
        '    function GetEntry(const Index: Integer): TIconEntry;'
        '{$ifend}'
        '  public'
        '    const _entries: array [0..%Count-1%] of TIconEntry =('
        '%value_declarations%'
        '    ); // _entries'
        '{$IFDEF Delphi2010_UP}'
        '%property_declarations%'
        '{$ELSE}'
        '%const_declarations%'
        '{$ifend}'
        '  end;'
        ''
        'var %ShortName%: %TypeName%;'
        ''
        'implementation'
        ''
        'function %TypeName%.GetEntry(const Index: Integer): TIconEntry;'
        'begin'
        '  Result := _entries[Index];'
        'end;'
        ''
        'initialization'
        '  TIconManager.Instance.AddCollection('
        
          '    TIconCollection.Create('#39'%name%'#39', '#39'%fontName%'#39', '#39'%description' +
          '%'#39', '#39'%version%'#39
        '    , procedure (const AEntryFunc: TIconEntryFunc)'
        '      var'
        '        LIndex: Integer;'
        '      begin'
        '        for LIndex := 0 to Length(%ShortName%._entries)-1 do'
        '        begin'
        '          if not AEntryFunc(%ShortName%._entries[LIndex]) then'
        '            Break;'
        '        end;'
        '      end'
        '    )'
        '  );'
        ''
        'end.'
        ''
        ''
        ''
        ''
        '')
